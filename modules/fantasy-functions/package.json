{
  "name": "@yafu/fantasy-functions",
  "version": "2.0.4",
  "license": "MIT",
  "author": "Ludwig Magnusson <ludwig.magnusson@gmail.com>",
  "type": "module",
  "exports": {
    "import": "./dist/es/fantasy-functions.js",
    "require": "./dist/cjs/fantasy-functions-development.cjs"
  },
  "main": "./dist/cjs/fantasy-functions-development.cjs",
  "types": "./dist/ts/fantasy-functions.d.ts",
  "files": [
    "lib",
    "dist",
    "readme.md",
    "index.js"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "generate": "pnpm generateEs & pnpm generateTs",
    "generateEs": "node scripts/generate-module.js",
    "generateTs": "node --loader ts-node/esm scripts/new-ts.ts > dist/ts/fantasy-functions.ts",
    "lint": "lint-yafu lib test *.js",
    "prepublishOnly": "pnpm clean && pnpm transpile && pnpm test",
    "test": "pnpm lint && pnpm unit",
    "transpile": "pnpm tsgen && rollup -c",
    "tsgen": "mkdir -p dist/ts && pnpm generate > dist/ts/fantasy-functions.ts",
    "unit": "mocha"
  },
  "dependencies": {
    "@yafu/curry": "^1.2.3",
    "@yafu/fantasy-types": "^1.0.4",
    "@yafu/type-utils": "^1.0.3",
    "fantasy-land": "^5.0.0",
    "yafu": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.7",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/node": "^18.0.3",
    "@yafu/eslint-config-yafu": "^1.1.2",
    "@yafu/rollup-plugin-curry": "^1.0.3",
    "babel-plugin-add-module-exports": "^1.0.4",
    "chai": "^4.3.6",
    "mocha": "^9.2.2",
    "rollup": "^2.70.1",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-ts": "^2.0.5",
    "ts-node": "^10.8.2",
    "typescript": "^4.6.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "6b481c79d7765e9358d1a0d215b566720f96f4ce"
}
