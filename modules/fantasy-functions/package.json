{
  "name": "@yafu/fantasy-functions",
  "version": "2.0.1",
  "license": "MIT",
  "author": "Ludwig Magnusson <ludwig.magnusson@gmail.com>",
  "exports": {
    "import": "./dist/es/fantasy-functions.js",
    "require": "./dist/cjs/fantasy-functions-development.cjs"
  },
  "main": "./dist/cjs/fantasy-functions-development.cjs",
  "types": "./dist/ts/fantasy-functions.d.ts",
  "type": "module",
  "files": [
    "lib",
    "dist",
    "readme.md",
    "index.js"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint lib test *.js",
    "prepublishOnly": "yarn clean && yarn transpile && yarn test",
    "test": "yarn lint && yarn unit",
    "generateEs": "node scripts/generate-module.js",
    "generateTs": "node --loader ts-node/esm scripts/new-ts.ts > dist/ts/fantasy-functions.ts",
    "generate": "yarn generateEs & yarn generateTs",
    "transpile": "yarn tsgen && rollup -c",
    "tsgen": "mkdir -p dist/ts && yarn generate > dist/ts/fantasy-functions.ts",
    "unit": "mocha"
  },
  "dependencies": {
    "@yafu/curry": "^1.2.1",
    "@yafu/fantasy-types": "^1.0.1",
    "fantasy-land": "^5.0.0",
    "yafu": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@yafu/eslint-config-yafu": "^1.1.0",
    "@yafu/rollup-plugin-curry": "^1.0.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "chai": "^4.3.6",
    "eslint": "^8.3.0",
    "mocha": "^9.2.0",
    "rollup": "^2.67.2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-ts": "^2.0.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "6b481c79d7765e9358d1a0d215b566720f96f4ce"
}
