{
  "name": "@yafu/fantasy-types",
  "version": "1.0.8",
  "description": "TypeScript defenitions of fantasy-land types",
  "license": "MIT",
  "author": "Ludwig Magnusson <ludwig.magnusson@gmail.com>",
  "type": "module",
  "exports": {
    "types": "./dist/types.d.ts",
    "import": "./dist/es/fantasy-types.js",
    "require": "./dist/cjs/fantasy-types.cjs"
  },
  "main": "./dist/cjs/fantasy-types.cjs",
  "types": "./dist/types.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "generate": "node dist/generate-ts-modules.js > dist/fantasy-types.ts",
    "lint": "tsc --noEmit dist/*.ts",
    "prepublishOnly": "pnpm clean && pnpm transpile && pnpm test",
    "test": "echo testless",
    "transpile": "rollup -c rollup.script.config.js && pnpm generate && rollup -c"
  },
  "dependencies": {
    "@yafu/curry": "^1.2.6",
    "@yafu/type-utils": "^1.0.6",
    "fantasy-land": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@yafu/create-rollup-config": "^1.1.3",
    "@yafu/rollup-plugin-curry": "^1.0.7",
    "esbuild": "^0.17.15",
    "rollup": "^3.20.2",
    "rollup-plugin-esbuild": "^5.0.0",
    "typescript": "^5.0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
