{
  "name": "@yafu/fantasy-types",
  "version": "1.0.4",
  "description": "TypeScript defenitions of fantasy-land types",
  "license": "MIT",
  "author": "Ludwig Magnusson <ludwig.magnusson@gmail.com>",
  "type": "module",
  "exports": {
    "import": "./dist/es/fantasy-types.js",
    "require": "./dist/cjs/fantasy-types.cjs"
  },
  "main": "./dist/cjs/fantasy-types.cjs",
  "types": "./dist/types.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "generate": "node dist/generate-ts-modules.js > dist/types.ts",
    "prepublishOnly": "pnpm clean && pnpm transpile && pnpm test",
    "test": "tsc --noEmit dist/*.ts",
    "transpile": "rollup -c rollup.script.config.js && pnpm generate && rollup -c"
  },
  "dependencies": {
    "@yafu/curry": "workspace:^1.2.3",
    "@yafu/type-utils": "workspace:^1.0.3",
    "fantasy-land": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@yafu/create-rollup-config": "workspace:^1.0.4",
    "@yafu/rollup-plugin-curry": "workspace:^1.0.3",
    "rollup": "^2.79.1",
    "rollup-plugin-ts": "^3.0.2",
    "typescript": "^4.8.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
