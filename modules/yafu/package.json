    "pretranspile": "mkdir -p dist",
    "transpile": "node build/create-index.js && rollup -c",
    "unit": "mocha testrunners/unit"
  },
  "dependencies": {
    "@yafu/curry": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
